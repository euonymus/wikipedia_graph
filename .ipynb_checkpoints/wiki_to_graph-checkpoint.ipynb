      "         [\n",
      "        </span>\n",
      "        <a href=\"/w/index.php?title=%E6%98%8E%E4%BB%81&amp;action=edit&amp;section=3\" title=\"節を編集: 発見したハゼ\">\n",
      "         編集\n",
      "        </a>\n",
      "        <span class=\"mw-editsection-bracket\">\n",
      "         ]\n",
      "        </span>\n",
      "       </span>\n",
      "      </h4>\n",
      "      <ul>\n",
      "       <li>\n",
      "        キマダラハゼ：\n",
      "        <i>\n",
      "         Astrabe flavimaculata\n",
      "        </i>\n",
      "        Akihito et Meguro\n",
      "        <sup class=\"reference\" id=\"cite_ref-32\">\n",
      "         <a href=\"#cite_note-32\">\n",
      "          [29]\n",
      "         </a>\n",
      "        </sup>\n",
      "       </li>\n",
      "       <li>\n",
      "        シマシロクラハゼ：\n",
      "        <i>\n",
      "         Astrabe fasciata\n",
      "        </i>\n",
      "        Akihito et Meguro\n",
      "       </li>\n",
      "       <li>\n",
      "        ヒメトサカハゼ：\n",
      "        <i>\n",
      "         Cristatogobius aurimaculatus\n",
      "        </i>\n",
      "        Akihito et Meguro\n",
      "       </li>\n",
      "       <li>\n",
      "        コンジキハゼ：\n",
      "        <i>\n",
      "         Glossogobius aureus\n",
      "        </i>\n",
      "        Akihito et Meguro\n",
      "       </li>\n",
      "       <li>\n",
      "        クロオビハゼ：\n",
      "        <i>\n",
      "         Myersina nigrivirgata\n",
      "        </i>\n",
      "        Akihito et Meguro\n",
      "       </li>\n",
      "       <li>\n",
      "        ミツボシゴマハゼ：\n",
      "        <i>\n",
      "         Pandaka trimaculata\n",
      "        </i>\n",
      "        Akihito et Meguro\n",
      "       </li>\n",
      "      </ul>\n",
